@model IEnumerable<StormWeb.Models.Student_Log>
@{
    ViewBag.Title = "Index";
}
<!-- JQuery UI and WIdget -->
<script src="@Url.Content("~/Scripts/jquery.widget.js")" type="text/javascript"></script>
<!--<link href="@Url.Content("~/Content/css/ui-lightness/jquery-ui-1.8.22.custom.css")" rel="stylesheet" type="text/css" />-->

<div class="row-fluid">
    <div class="span12">
        <div class="widget-block">
            <div class="widget-head">
                <h5>
                    Log list</h5>
                
            </div>
            <div class="widget-content">
                <div class="widget-box">
                    <table class="data-tbl-boxy table">
                        <thead>
                            <tr>
                            <th>
                                  UserName
                                   
                                </th>
                                <th>
                                  DateTime
                                   
                                </th>
                                <th>
                                  Comment
                                </th>
                                <th>
                                    Section
                                </th>
                                <th>
                                    Type
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                 <td>
                                        @Html.DisplayFor(modelItem => item.UserName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.DateTime)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Comment)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Section)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Type)
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    @*<p>
        @Html.ActionLink("Create New", "Create")
    </p>*@
</div>
